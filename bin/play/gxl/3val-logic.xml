<!-- 3-valued logic -->

<gxl xmlns:xbel='www.cs.toronto.edu/xbel'
     xmlns:xlink='xlink'>
 <graph ID='3-val' edgemode='directed'>
   <xbel:img xlink:type='simple' xlink:href='play/3val.gif'/>
   <!-- logic values -->
   <node ID='T'/>
   <node ID='M'/>
   <node ID='F'/>

   <!-- logic order -->
   <edge from='T' to='M'>
    <type value='above'/>
   </edge>

   <edge from='M' to='F'>
    <type value='above'/>
   </edge>
 
   <!-- negation order -->
   <edge from='T' to='F'>
     <type value='neg'/>
   </edge>

   <edge from='F' to='T'>
     <type value='neg'/>
   </edge>

   <edge from='M' to='M'>
     <type value='neg'/>
   </edge>

 </graph>
</gxl>
